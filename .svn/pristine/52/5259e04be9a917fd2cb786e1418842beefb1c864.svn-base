package com.aotain.common.utils.model.msg;

import com.aotain.common.utils.model.msg.BaseVo;

/**
 * 指令和策略消息
 * 
 * @author silence
 * @time 2018年3月6日
 */
public class CommandMessage extends BaseVo {

	private static final long serialVersionUID = 1516483932067229926L;
	private Long statId;// 统计ID
	private Long toptaskid;// 所属任务ID
	private String houseidstr;// 机房编码值
	private Integer command_type;// 指令类型
	private Integer policy_type;// 策略类型
	private Integer data_type;// 数据类型，1-指令 2-策略
	private Long statis_time;// 指令或策略的下发创建时间， UTC格式
	private Integer status;// 指令或者策略的下发状态，0-成功 1-失败
	private Long commandid;// 指令ID
	private Long message_no;// 策略的消息号
	private Long createtime;// 创建时间， UTC格式

	public Long getStatId() {
		return statId;
	}

	public void setStatId(Long statId) {
		this.statId = statId;
	}

	public Long getToptaskid() {
		return toptaskid;
	}

	public void setToptaskid(Long toptaskid) {
		this.toptaskid = toptaskid;
	}

	public String getHouseidstr() {
		return houseidstr;
	}

	public void setHouseidstr(String houseidstr) {
		this.houseidstr = houseidstr;
	}

	public Integer getCommand_type() {
		return command_type;
	}

	public void setCommand_type(Integer command_type) {
		this.command_type = command_type;
	}

	public Integer getPolicy_type() {
		return policy_type;
	}

	public void setPolicy_type(Integer policy_type) {
		this.policy_type = policy_type;
	}

	public Integer getData_type() {
		return data_type;
	}

	public void setData_type(Integer data_type) {
		this.data_type = data_type;
	}

	public Long getStatis_time() {
		return statis_time;
	}

	public void setStatis_time(Long statis_time) {
		this.statis_time = statis_time;
	}

	public Integer getStatus() {
		return status;
	}

	public void setStatus(Integer status) {
		this.status = status;
	}

	public Long getCommandid() {
		return commandid;
	}

	public void setCommandid(Long commandid) {
		this.commandid = commandid;
	}

	public Long getMessage_no() {
		return message_no;
	}

	public void setMessage_no(Long message_no) {
		this.message_no = message_no;
	}

	public Long getCreatetime() {
		return createtime;
	}

	public void setCreatetime(Long createtime) {
		this.createtime = createtime;
	}

}
